      const userName = 'kdabra';
    const conversation  = {
      user : {
          userId : 'fedeaperez',
          userName: 'Federico'
      },
      lastMessage :  {
          text : 'Perfecto, me queda bien ese horario!',
          date : '14:58'
      },
      relationship : {
          text : 'Fan',
          color : 'orange'
      }
    }
    const conversation_dos  = {
      user : {
        userId:'jorgefcarlos',
          userName: 'Jorge'
      },
      lastMessage :  {
          text : 'Mañana a la tarde te llevo el pedido ',
          date : '12:08'
      },
      relationship : {
